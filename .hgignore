# use glob syntax
syntax: glob

*.class
*.log
*.swp
.#*
.DS_Store

# ignore Maven build directories and Eclipse, Netbeans and IDEA project files
target
.classpath
.project
.settings
*.iml
*.ipr
*.iws
.idea
nbactions.xml

# switch to regexp syntax
syntax: regexp
\.orig$
\.orig\..*$
\.chg\..*$
\.rej$
\.conflict\~$
