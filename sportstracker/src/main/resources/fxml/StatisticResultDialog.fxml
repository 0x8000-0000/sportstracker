<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.embed.swing.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="16.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.saring.sportstracker.gui.dialogsfx.StatisticResultDialogController">
   <children>
      <HBox alignment="CENTER_LEFT">
         <children>
            <Label maxWidth="1.7976931348623157E308" text="%st.dlg.statistic_results.total.text" HBox.hgrow="ALWAYS">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets right="2.0" />
         </padding>
      </HBox>
      <GridPane hgap="16.0" vgap="10.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="160.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints vgrow="SOMETIMES" />
            <RowConstraints vgrow="SOMETIMES" />
            <RowConstraints vgrow="SOMETIMES" />
          <RowConstraints vgrow="SOMETIMES" />
          <RowConstraints vgrow="SOMETIMES" />
          <RowConstraints vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="%st.dlg.statistic_results.total_exercises.text" />
            <Label fx:id="laTotalExercisesValue" text="%common.n_a_" GridPane.columnIndex="1" />
            <Label text="%st.dlg.statistic_results.total_distance.text" GridPane.rowIndex="1" />
            <Label fx:id="laTotalDistanceValue" text="%common.n_a_" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label text="%st.dlg.statistic_results.total_duration.text" GridPane.rowIndex="2" />
            <Label fx:id="laTotalDurationValue" text="%common.n_a_" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label text="%st.dlg.statistic_results.total_ascent.text" GridPane.rowIndex="3" />
            <Label fx:id="laTotalAscentValue" text="%common.n_a_" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label text="%st.dlg.statistic_results.calories.text" GridPane.rowIndex="4" />
            <Label fx:id="laTotalEnergyValue" text="%common.n_a_" GridPane.columnIndex="1" GridPane.rowIndex="4" />
         </children>
         <VBox.margin>
            <Insets left="24.0" />
         </VBox.margin>
      </GridPane>
   </children>
</VBox>
