<?xml version="1.0" encoding="utf-8"?>
<jnlp spec="1.0+" codebase="http://www.saring.de/sportstracker/webstart" href="sportstracker.jnlp">
    <information>
        <title>SportsTracker</title>
        <vendor>Stefan Saring</vendor>
        <homepage href="http://www.saring.de/sportstracker"/>
        <description kind="short">A tool for recording your sport activities and viewing exercise files.</description>
        <icon href="st-logo.png"/>
        <offline-allowed/>
